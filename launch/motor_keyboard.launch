<launch>
	<node name="driver_control" pkg="rosserial_python" type="serial_node.py" output="screen">
	            <param name="port" value="/dev/ttyACM0"/>
	            <param name="baud" value="57600" />
	</node>

	<node name="sensor_feedback" pkg="rosserial_python" type="serial_node.py" output="screen">
	            <param name="port" value="/dev/ttyACM1"/>
	            <param name="baud" value="57600" />
	</node>


	<node name="plant" pkg="mower" type="driveclass.py" output="screen"/>

	<node name="recorder" pkg="rosbag" type="record" args="record -o /home/nvidia/Desktop/bagfile/ /rosout"/>


</launch>
